<cfset handler = event.getCurrentHandler()><cfset htmlHelper = getPlugin('HtmlHelper')><cfset baseURL = getSetting('baseurl')><cfoutput><!DOCTYPE html><html><head>    <title>Toys for Shots - Toys For Tots Annual Event</title>	<meta http-equiv="Content-Type" content="text/html;charset=ISO-8859-1" />	<meta name="description" content="Toys For Shots is an annual funraiser held in Cleveland Ohio." />	<meta name="keywords" content="toys for shots,toys for tots,cleveland,oh,fundraiser,event,dan vega" />    <!--[if lt IE 9]><script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script><![endif]-->	<link href='http://fonts.googleapis.com/css?family=Lobster' rel='stylesheet' type='text/css'>	<link href='http://fonts.googleapis.com/css?family=Yanone+Kaffeesatz&subset=latin' rel='stylesheet' type='text/css'>	<link href='http://fonts.googleapis.com/css?family=Droid+Sans' rel='stylesheet' type='text/css'>	<link href='http://fonts.googleapis.com/css?family=Dancing+Script' rel='stylesheet' type='text/css'>	<!---<link href="http://twitter.github.com/bootstrap/assets/css/bootstrap-1.2.0.min.css" rel="stylesheet" type="text/css">--->	#htmlHelper.link(href='includes/bootstrap/css/bootstrap.min.css')#	#htmlHelper.link(href='includes/css/stylesheet.css')#	<script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1/jquery.min.js"></script>	<script type="text/javascript">		$(function() {		});		try {		    var _gaq = _gaq || [];		 	_gaq.push(['_setAccount', '#getSetting('ga_account')#']);			_gaq.push(['_setDomainName', '.toysforshots.com' ]);			_gaq.push(['_setAllowHash', false]);			_gaq.push(['_setAllowLinker', true]);		    _gaq.push(['_trackPageview']);		    _gaq.push(['_trackPageLoadTime']);			(function() {				var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;				ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';				var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);			})();		}		catch (e) {		}	</script></head><body>	<div class="container">		<div class="content-wrapper">			<div class="row">				<div id="header">					<a href="/">#htmlHelper.img(src='includes/images/toysforshotslogo.jpg',width='220',border='0',class='logo')#</a>				</div>			</div>			<div class="row">				<div id="navigation" class="span12">					<ul>						<li><a href="#event.buildLink('')#" class="<cfif handler EQ 'main'>current</cfif>">Home</a></li>						<li><a href="#event.buildLink('about')#" class="<cfif handler EQ 'about'>current</cfif>">About</a></li>						<li><a href="#event.buildLink('donate')#" class="<cfif handler EQ 'donate'>current</cfif>">Donations</a></li>						<li><a href="#event.buildLink('raffle')#" class="<cfif handler EQ 'raffle'>current</cfif>">Raffle</a></li>						<li><a href="#event.buildLink('gallery')#" class="<cfif handler EQ 'gallery'>current</cfif>">Gallery</a></li>						<li><a href="#event.buildLink('rsvp')#" class="<cfif handler EQ 'rsvp'>current</cfif>">RSVP</a></li>						<li><a href="/blog" class="<cfif handler EQ 'blog'>current</cfif>">Blog</a></li>						<li><a href="#event.buildLink('thankyou')#" class="<cfif handler EQ 'thankyou'>current</cfif>">Thank You</a></li>						<li><a href="#event.buildLink('contact')#" class="<cfif handler EQ 'contact'>current</cfif>">Contact Us</a></li>					</ul>				</div>			</div>			#renderView()#		</div>		<div id="footer" class="row">			Please <a href="/contact">contact us</a> if you have any questions about the site, the event or the owner of this website</a>.		</div>	</div></body></html></cfoutput>